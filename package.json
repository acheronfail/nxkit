{
  "type": "module",
  "name": "nxkit",
  "productName": "NKXit",
  "version": "1.0.0",
  "description": "Utilities for Nintendo Switch (NX) Homebrew",
  "main": ".vite/build/main.js",
  "scripts": {
    "start": "electron-forge start",
    "package": "electron-forge package",
    "make": "electron-forge make",
    "publish": "electron-forge publish",
    "lint": "eslint --ext .ts,.tsx .",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "test": "node scripts/npm-run.js test:vitest:run test:types:tsc test:types:svelte format:check lint",
    "test:vitest": "vitest --config vite.test.config.ts",
    "test:vitest:run": "vitest --config vite.test.config.ts run",
    "test:types:tsc": "tsc -p tsconfig.json",
    "test:types:svelte": "svelte-check --tsconfig tsconfig.json",
    "postinstall": "scripts/hacks.js"
  },
  "devDependencies": {
    "@electron-forge/cli": "^7.4.0",
    "@electron-forge/maker-deb": "^7.4.0",
    "@electron-forge/maker-rpm": "^7.4.0",
    "@electron-forge/maker-squirrel": "^7.4.0",
    "@electron-forge/maker-zip": "^7.4.0",
    "@electron-forge/plugin-auto-unpack-natives": "^7.4.0",
    "@electron-forge/plugin-fuses": "^7.4.0",
    "@electron-forge/plugin-vite": "^7.4.0",
    "@electron/fuses": "^1.8.0",
    "@sveltejs/vite-plugin-svelte": "^3.1.1",
    "@testing-library/jest-dom": "^6.4.6",
    "@testing-library/svelte": "^5.1.0",
    "@types/emscripten": "^1.39.13",
    "@types/w3c-web-usb": "^1.0.10",
    "@typescript-eslint/eslint-plugin": "^5.0.0",
    "@typescript-eslint/parser": "^5.0.0",
    "autoprefixer": "^10.4.19",
    "chalk": "^5.3.0",
    "electron": "30.1.0",
    "eslint": "^8.0.1",
    "eslint-import-resolver-typescript": "^3.6.1",
    "eslint-plugin-import": "^2.25.0",
    "jsdom": "^24.1.0",
    "postcss": "^8.4.38",
    "prettier": "^3.3.2",
    "prettier-plugin-svelte": "^3.2.4",
    "svelte": "^5.0.0-next.155",
    "svelte-check": "^3.8.1",
    "tailwindcss": "^3.4.4",
    "ts-node": "^10.0.0",
    "tsx": "^4.15.6",
    "typescript": "^5.4.5",
    "vite": "^5.0.12",
    "vite-plugin-wasm": "^3.3.0",
    "vitest": "^1.6.0"
  },
  "keywords": [],
  "author": "acheronfail",
  "license": "AGPL-3.0-only",
  "dependencies": {
    "@floating-ui/dom": "^1.6.5",
    "@tootallnate/nacp": "^0.0.1",
    "@tootallnate/nro": "^0.1.0",
    "electron-context-menu": "^4.0.0",
    "electron-squirrel-startup": "^1.0.1",
    "gpt": "^3.0.0",
    "heroicons-svelte": "^2.0.0",
    "js-fatfs": "^0.1.0",
    "mbr": "^1.1.3",
    "pretty-bytes": "^6.1.1",
    "svelte-easy-crop": "^2.0.3",
    "zod": "^3.23.8"
  }
}
